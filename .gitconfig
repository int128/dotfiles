[merge]
  ff = false
[pull]
  rebase = true
[push]
  default = tracking
[alias]
  ci = commit
  co = checkout
  ls = log --graph --decorate --oneline
  ll = log --stat --decorate --oneline --find-renames
  st = status --short --branch
  pushoriginmaster = push origin master
  fetchorigin = fetch origin

